<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.PieChart?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="450.0" prefWidth="750.0"
            style="-fx-background-color: #DECEEB;" xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="controllers.admin.CheckResultsController">
    <Label layoutX="104.0" layoutY="13.0" text="Select Position" textFill="#6b27a7">
        <font>
            <Font name="OCR A Extended" size="25.0"/>
        </font>
    </Label>
    <ComboBox fx:id="selectPosition" layoutX="375.0" layoutY="14.0" onAction="#makePieChart" prefWidth="150.0"
              style="-fx-background-color: #A074C7;"/>
    <PieChart fx:id="nameChart" clockwise="false" layoutX="25.0" layoutY="80.0" legendVisible="false" prefHeight="322.0"
              prefWidth="350.0" titleSide="LEFT"/>
    <PieChart fx:id="numberChart" clockwise="false" labelLineLength="40.0" layoutX="375.0" layoutY="80.0"
              legendVisible="false" prefHeight="322.0" prefWidth="350.0" titleSide="BOTTOM"/>
    <Label fx:id="winner" alignment="CENTER" layoutX="25.0" layoutY="408.0" prefHeight="28.0" prefWidth="700.0"
           text="Winner Name Here" textFill="#6b27a7">
        <font>
            <Font name="OCR A Extended" size="25.0"/>
        </font>
    </Label>
</AnchorPane>
